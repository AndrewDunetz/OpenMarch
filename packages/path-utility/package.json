{
    "name": "@openmarch/path-utility",
    "version": "1.0.0",
    "description": "A TypeScript library for working with complex paths that preserves spline segment data in JSON serialization",
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "clean": "rm -rf dist",
        "prebuild": "npm run clean",
        "test": "vitest run --silent",
        "test:watch": "vitest",
        "dev": "tsc --watch"
    },
    "keywords": [
        "path",
        "spline",
        "svg",
        "geometry",
        "typescript"
    ],
    "author": "OpenMarch",
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/uuid": "^10.0.0",
        "typescript": "^5.0.0",
        "vitest": "^3.0.0"
    },
    "files": [
        "dist/",
        "README.md",
        "LICENSE"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "dependencies": {
        "svg-path-commander": "^2.1.11",
        "uuid": "^11.1.0"
    }
}
