{
    "name": "@openmarch/ui",
    "type": "module",
    "version": "0.0.0",
    "license": "GPL-3.0",
    "module": "./src/index.ts",
    "types": "./src/index.ts",
    "exports": {
        ".": {
            "types": "./src/index.ts",
            "import": "./src/index.ts",
            "default": "./src/index.ts"
        },
        "./astro": {
            "types": "./src/astro.ts",
            "import": "./src/astro.ts",
            "default": "./src/astro.ts"
        },
        "./src/tailwind.css": {
            "default": "./src/tailwind.css"
        }
    },
    "scripts": {
        "dev": "astro dev",
        "build": "astro check && astro build",
        "preview": "astro preview",
        "astro": "astro",
        "lint": "eslint . --fix",
        "format": "prettier --write .",
        "spellcheck": "cspell 'src/**' 'electron/**' '*.md'",
        "fix": "prettier --write . && eslint . --fix && cspell 'src/**' 'electron/**' '*.md'"
    },
    "dependencies": {
        "@astrojs/mdx": "^4.2.6",
        "@astrojs/react": "^4.2.7",
        "@fontsource/dm-mono": "^5.2.5",
        "@fontsource/dm-sans": "^5.2.5",
        "@korhq/unpreview": "0.1.2-alpha",
        "@phosphor-icons/react": "^2.1.8",
        "@radix-ui/react-alert-dialog": "^1.1.13",
        "@radix-ui/react-checkbox": "^1.3.1",
        "@radix-ui/react-dialog": "^1.1.13",
        "@radix-ui/react-radio-group": "^1.3.6",
        "@radix-ui/react-select": "^2.2.4",
        "@radix-ui/react-switch": "^1.2.4",
        "@radix-ui/react-toggle-group": "^1.1.10",
        "@radix-ui/react-tabs": "^1.1.12",
        "@radix-ui/react-tooltip": "^1.2.6",
        "@tailwindcss/vite": "^4.1.7",
        "@types/react": "^18.3.21",
        "@types/react-dom": "^18.3.7",
        "@uiw/react-color": "^2.5.5",
        "astro": "^5.7.13",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "^4.1.7"
    }
