{
    "name": "openmarch",
    "productName": "OpenMarch",
    "version": "0.0.2",
    "main": "dist-electron/main/index.js",
    "description": "Electron Vite React boilerplate.",
    "author": "Alex Dumouchelle",
    "license": "GPL-3.0",
    "private": true,
    "debug": {
        "env": {
            "VITE_DEV_SERVER_URL": "http://127.0.0.1:7777/"
        }
    },
    "scripts": {
        "dev": "eslint . && vite",
        "build-mac": "tsc && vite build && electron-builder --mac ",
        "build-win": "tsc && vite build && electron-builder --win",
        "preview": "vite preview",
        "pree2e": "vite build --mode=test",
        "e2e": "playwright test",
        "postinstall": "electron-rebuild && npm rebuild canvas --update-binary",
        "lint": "eslint .",
        "test": "eslint . && vitest",
        "test:watch": "eslint . && vitest --watch",
        "test:coverage": "eslint . && vitest --coverage",
        "test:clearCache": "vitest --clearCache"
    },
    "dependencies": {
        "autoprefixer": "^10.4.19",
        "better-sqlite3": "^9.6.0",
        "electron-store": "^8.1.0",
        "electron-updater": "^6.2.1",
        "fabric": "^5.3.0",
        "jquery": "^3.7.1",
        "nan": "^2.19.0",
        "postcss": "^8.4.38",
        "puppeteer": "^22.13.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^4.12.0",
        "sass": "^1.70.0",
        "xml2abc": "file:external-packages/xml2abc.tgz",
        "zustand": "^4.4.7"
    },
    "devDependencies": {
        "@electron/rebuild": "^3.6.0",
        "@playwright/test": "^1.43.1",
        "@testing-library/jest-dom": "^6.4.8",
        "@testing-library/react": "^15.0.6",
        "@testing-library/user-event": "^14.5.2",
        "@types/better-sqlite3": "^7.6.8",
        "@types/fabric": "^5.3.6",
        "@types/node": "^20.10.5",
        "@types/react": "^18.2.20",
        "@types/react-dom": "^18.2.7",
        "@typescript-eslint/parser": "^6.21.0",
        "@vitejs/plugin-react": "^4.0.4",
        "electron": "^29.3.3",
        "electron-builder": "^24.6.3",
        "eslint": "^8.56.0",
        "eslint-config-react-app": "^7.0.1",
        "eslint-plugin-react": "^7.33.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "jsdom": "^24.1.1",
        "tailwindcss": "^3.4.3",
        "typescript": "5.3",
        "vite": "^4.4.9",
        "vite-plugin-electron": "^0.15.5",
        "vite-plugin-electron-renderer": "^0.14.5",
        "vitest": "^2.0.5"
    },
    "engines": {
        "node": ">18.0.0"
    }
}
